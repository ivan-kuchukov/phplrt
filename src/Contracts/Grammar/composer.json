{
    "name": "phplrt/grammar-contracts",
    "type": "library",
    "description": "A set of phplrt grammar abstractions",
    "license": "MIT",
    "keywords": ["abstractions", "contracts", "decoupling", "interfaces", "interoperability", "standards", "grammar", "rules"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/grammar-contracts/issues",
        "source": "https://github.com/phplrt/grammar-contracts"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phplrt/lexer-contracts": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Contracts\\Grammar\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
