{
    "name": "phplrt/trace",
    "type": "library",
    "description": "The phplrt trace package.",
    "license": "MIT",
    "keywords": [
        "trace",
        "visualization",
        "position",
        "backtrace",
        "exception",
        "debig"
    ],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/trace"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "phplrt/trace-contracts": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Trace\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Trace\\Tests\\": "tests"
        }
    },
    "provide": {
        "phplrt/trace-contracts-implementation": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
