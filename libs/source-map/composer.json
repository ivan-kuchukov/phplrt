{
    "name": "phplrt/source-map",
    "type": "library",
    "description": "The phplrt source-map package",
    "license": "MIT",
    "keywords": ["file", "sourcemap", "source-map", "mapping", "transpiling", "transform"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/source-map"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\SourceMap\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\SourceMap\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
