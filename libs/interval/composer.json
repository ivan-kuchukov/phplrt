{
    "name": "phplrt/interval",
    "type": "library",
    "description": "The phplrt interval package.",
    "license": "MIT",
    "keywords": [
        "analyzing",
        "visualization",
        "input",
        "position",
        "interval",
        "line",
        "column",
        "offset",
        "utils"
    ],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/interval"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "phplrt/interval-contracts": "^4.0",
        "phplrt/source": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Interval\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Interval\\Tests\\": "tests"
        }
    },
    "provide": {
        "phplrt/interval-contracts-implementation": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
